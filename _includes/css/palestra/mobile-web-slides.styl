.iphone
	background: #333
	border-radius: 2em
	position: relative
	
	&.portrait
		width: 15em
		height: 28.125em;

		&:before, .hotzone, .screen, .browser
			absolute: left 0.625em top 4.375em
			height: 18.375em
			width: 13.75em

		&:after
			absolute: left 6.0625em bottom 1.25em

		.hotzone
			&:before
				content: ''
				absolute: left -11.875em top -2.5em
				border-style: solid
				border-width: 12.1875em
				border-radius: 62.5em

				height: 62.5em
				width: 62.5em

	&.landscape
		width: 28.125em
		height: 15em

		&:before, .hotzone, .screen, .browser
			absolute: left 4.375em top 0.625em
			height: 13.75em
			width: 18.375em

		&:after
			absolute: right 1.25em bottom 6.0625em

		.hotzone
			&:before, &:after
				content: ''
				absolute: left -19.5em top -6.875em
				border-style: solid
				border-width: 13.75em
				border-radius: 13.75em
			&:after
				left: auto
				right: -19.5em

	&:before
		content: ''
		background: #555

	&:after
		content: ''
		border: 1.5625em solid #555
		border-radius: 1.5625em

	.hotzone
		overflow: hidden

		&:before, &:after
			border-color: amarelo

	.screen
		background: white
		overflow: hidden

	.browser
		background-color: laranja
		background-size: 15px 15px

	.browser
		border-top: 0.8em solid #bbb
		border-bottom: 1.5em solid azul
		position: relative

		&:before // address bar
			content: ''
			absolute: top left
			height: 2.2em
			width: 100%
			box-sizing: border-box

			background: white
			border: 0.5em solid azul

	@css {
		.iphone .browser {
			background-image: -webkit-gradient(linear, 0 100%, 100% 0, color-stop(.25, rgba(255, 255, 255, .2)), color-stop(.25, transparent), color-stop(.5, transparent), color-stop(.5, rgba(255, 255, 255, .2)),color-stop(.75, rgba(255, 255, 255, .2)), color-stop(.75, transparent), to(transparent));
			background-image: -webkit-linear-gradient(45deg, rgba(255, 255, 255, .2) 25%, transparent 25%, transparent 50%, rgba(255, 255, 255, .2) 50%, rgba(255, 255, 255, .2) 75%, transparent 75%, transparent);
			background-image: -moz-linear-gradient(45deg, rgba(255, 255, 255, .2) 25%, transparent 25%, transparent 50%, rgba(255, 255, 255, .2) 50%, rgba(255, 255, 255, .2) 75%, transparent 75%, transparent);
			background-image: -ms-linear-gradient(45deg, rgba(255, 255, 255, .2) 25%, transparent 25%, transparent 50%, rgba(255, 255, 255, .2) 50%, rgba(255, 255, 255, .2) 75%, transparent 75%, transparent);
			background-image: -o-linear-gradient(45deg, rgba(255, 255, 255, .2) 25%, transparent 25%, transparent 50%, rgba(255, 255, 255, .2) 50%, rgba(255, 255, 255, .2) 75%, transparent 75%, transparent);
			background-image: linear-gradient(45deg, rgba(255, 255, 255, .2) 25%, transparent 25%, transparent 50%, rgba(255, 255, 255, .2) 50%, rgba(255, 255, 255, .2) 75%, transparent 75%, transparent);
		}
	}

