@keyframes desce {
	0% { transform: translateX(450px); }
	80% { transform: translateX(450px); }
	100% { transform: translateX(0px); }
}
@keyframes desce-lento {
	0% { transform: translateX(450px); }
	90% { transform: translateX(450px); }
	100% { transform: translateX(0px); }
}
@keyframes desce-mais-lento {
	0% { transform: translateX(450px); }
	95% { transform: translateX(450px); }
	100% { transform: translateX(0px); }
}

@keyframes lightSpeedIn {
	0% { transform: translateX(100%) skewX(-30deg); opacity: 0; }
	60% { transform: translateX(-20%) skewX(30deg); opacity: 1; }
	80% { transform: translateX(0%) skewX(-15deg); opacity: 1; }
	100% { transform: translateX(0%) skewX(0deg); opacity: 1; }
}
@keyframes rollIn {
	0% { opacity: 0; transform: translateX(-100%) rotate(-120deg); }
	50% { opacity: 0; transform: translateX(-100%) rotate(-120deg); }
	100% { opacity: 1; transform: translateX(0px) rotate(0deg); }
}
